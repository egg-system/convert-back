{
  "name": "convert-csv-layer",
  "version": "1.0.0",
  "description": "convert-csv-service-lamda's package",
  "author": "x-faCE",
  "private": true,
  "scripts": {
    "build": "npm run forceInstall && npm run makeNodeDir && npm run makeZip && rm -rf ./nodejs",
    "forceInstall": "rm -rf ./node_modules && npm install",
    "makeNodeDir": "mkdir -p ./nodejs && cp -r ./node_modules ./nodejs/node_modules",
    "makeZip": "zip -r convert-layer.zip ./nodejs/"
  },
  "dependencies": {
    "csv": "^5.3.2",
    "storage": "file:storage"
  }
}
